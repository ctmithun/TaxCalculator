<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<jsp-config>
		<taglib>
			<taglib-uri>/WEB-INF/security.tld</taglib-uri>
			<taglib-location>/WEB-INF/security.tld</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>/WEB-INF/spring.tld</taglib-uri>
			<taglib-location>/WEB-INF/spring.tld</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>/WEB-INF/spring-form.tld</taglib-uri>
			<taglib-location>/WEB-INF/spring-form.tld</taglib-location>
		</taglib>
	</jsp-config>
</web-app>
